!SETTINGS

!
! URXVT
!

!URxvt.font: xft:SauceCodePro Nerd Font:style=Regular:size=9:antialias=false
!URxvt.fontbold: xft:SauceCodePro Nerd Font:style=Bold:size=9:antialias=false

!URxvt.font: xft:TerminessTTF Nerd Font:style=Medium:size=9:antialias=false
!URxvt.fontbold: xft:TerminessTTF Nerd Font:style=Bold:size=9:antialias=false

!URxvt.font: xft:xos4 Terminess Powerline:style=Regular:size=9
!URxvt.fontbold: xft:xos4 Terminess Powerline:style=Bold:size=9

URxvt.font: xft:Knack Nerd Font:style=Regualr:size=9
URxvt.fontbold: xft:Knack Nerd Font:style=Bold:size=9

!URxvt.font: xft:Iosevka Term:style=Regualr:size=9
!URxvt.fontbold: xft:Iosevka Term:style=Bold:size=9

!URxvt.font: xft:xos4 Terminus:style=Regular:size=9
!URxvt.fontbold: xft:xos4 Terminus:style=Bold:size=9

!URxvt.font: xft:ProFontIIx Nerd Font:style=Book:size=8
!URxvt.fontbold: xft:GohuFontMedium Nerd Font:style=Bold:size=10

!URxvt.font: xft:Knack Nerd Font:style=Regular:size=9:antialias=false
!URxvt.fontbold: xft:Knack Nerd Font:style=Bold:size=9:antialias=false

!URxvt.font: xft:Input Mono:style=Regular:size=9
!URxvt.fontbold: xft:GohuFontMedium Nerd Font:style=Bold:size=10

!URxvt.font: xft:FantasqueSansMono Nerd Font:style=Regular:size=10
!URxvt.fontbold: xft:FantasqueSansMono Nerd Font:style=Bold:size=10

! URxvt settings
!URxvt*dynamicColors: on
URxvt.internalBorder: 15
URxvt*lineSpace: 1
URxvt*scrollBar: false
URxvt*scrollColor: #131415
URxvt.geometry: 80x18
URxvt*saveLines: 1000
URxvt*cursorUnderline: false
URxvt*cursorColor: #d79921
URxvt*cursorBlink: true
URxvt*underlineColor: #ebdbb2
URxvt*allow_bold: true


! Disables scrollback buffer in secondary screen
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

! Clickable and colorful URLs
URxvt.perl-ext-common: default,matcher,clipboard,tabbed
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg16

! Tabs colors
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

! Enables copy/paste with Ctrl-Shift-C/V
URxvt.iso14755: false
URxvt.iso14755_52: false
!URxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

! xst settings
st.shell: /usr/bin/zsh
st.font: Knack Nerd Font:size=10

! Xterm settings

xterm*faceName: xos4 Terminess Powerline:style=Regular:size=10
xterm*VT100.Translations: #override Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n Ctrl Shift <Key>C: copy-selection(CLIPBOARD)


!
! Colors
!

! arc blue background
!*background: #1f222d

! hard contrast: *background: #1d2021
! soft contrast: *background: #32302f

*background: #282828
*foreground: #ebdbb2

! Black + DarkGrey
*color0:  #282828
*color8:  #928374

! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934

! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26

! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f

! DarkBlue + Blue
*color4:  #458588
*color12: #83a598

! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b

! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c

! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2


! Cursor theme
Xcursor.theme: OSX-ElCap

! !cursor
*cursorColor: #d79921

! Xft settings
Xft.dpi: 100
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
!Xft.lcdfilter:  lcdlight
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

